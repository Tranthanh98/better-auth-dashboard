{
  "name": "better-auth-admin",
  "version": "0.1.0",
  "description": "Express adapter for Better Auth Admin Dashboard",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "client"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "build:client": "cd ../../client && pnpm build && cp -r build/client ../../packages/express-adapter/client",
    "prepublishOnly": "pnpm build && pnpm build:client",
    "dev": "tsup src/index.ts --format esm --dts --watch"
  },
  "keywords": [
    "better-auth",
    "admin",
    "dashboard",
    "express",
    "middleware"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "express": "^4.21.0",
    "tsup": "^8.3.0",
    "typescript": "^5.9.2"
  }
}
