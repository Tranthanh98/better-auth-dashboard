{
  "name": "better-auth-admin",
  "version": "0.1.3",
  "description": "Admin Dashboard for Better Auth - Manage users, organizations, and sessions",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "client"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "build:client": "cd ../../client && pnpm build && node ../packages/express-adapter/scripts/copy-client.js",
    "prepublishOnly": "pnpm build && pnpm build:client",
    "dev": "tsup src/index.ts --format esm --dts --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tranthanh98/better-auth-dashboard"
  },
  "homepage": "https://github.com/Tranthanh98/better-auth-dashboard#readme",
  "bugs": {
    "url": "https://github.com/Tranthanh98/better-auth-dashboard/issues"
  },
  "keywords": [
    "better-auth",
    "admin",
    "dashboard",
    "express",
    "middleware",
    "user-management",
    "organization-management",
    "session-management"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "express": "^4.21.0",
    "tsup": "^8.3.0",
    "typescript": "^5.9.2"
  }
}
